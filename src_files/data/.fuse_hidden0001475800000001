                        lvl="hallwayCHASE-8"
                        x=-21
                    }
                    if(y<=37){
                        lvl="hallwayCHASE-9"
                        y=74
                        x=-21
                    }
                    if(y<=89&&x<=204){
                        y=86
                    }
                }
