            function collide(){
                if(lvl=="garage_cece1_closet"){
                    if(x<=75){
                        x=76
                    }
                    if(y<=47){
                        y=48
                    }
                    if(x>=240 && y<=80){
                        x=239
                    }
                    if(x>=213 && y>=80){
                        x=212
                    }
                    if(y>=83 && x<=111){
                        y=82
                    }
                    if(y>=89 && x>=111){
                        y=88
                    }
                }
                if(lvl=="garden_minigame"){
                    if(x>=459){
                        x=458
                    }
                    if(y>31&&y<151&&x<-79){
                        x=-78
                    }
                    if(x<=-123){
                        x=-124
                    }
                    if(y<=-58){
                        y=-59
                    }
                    if(y>=167){
                        y=166
                    }
                }
                if(lvl=="outsidegame_cece1_backyard"){
                    if(y<=35){
                        y=36
                    }
                    if(y>=105){
                        y=104
                    }
                    if(x>=273){
                        x=272
                    }
                    if(x<=-6){
                        x=-5
                    }
                }
                if(lvl=="garage_cece1"){
                    if(x<=45){
                        x=46
                    }
                    if(x>=246){
                        x=245
                    }
                    if(y<=23){
                        y=24
                    }
                    if(y>=92){
                        y=91
                    }
                }
                if(lvl=="ghntb"){
                    if(y<=14){
                        y=15
                    }
                    if(y>=44){
                        if(x>=33 && x<=66){

                        }else{
                            y=43
                        }
                    }
                    if(y>=84){
                        y=83
                    }
                    if(x>=234){
                        x=233
                    }
                    if(x<=-22){
                        lvl="hallway_backyard_connect1"
                        x=274
                        y=64
                    }
                }
                if(lvl=="locked_hallway_side1"){
                    if(y>=65){
                        y=64
                    }
                    if(y<=35){
                        y=36
                    }
                    if(x<=234 && tempINFO[2]!=7){
                        x=235
                    }

                    if(x>=289){
                        lvl="t_hallway1"
                        x=-1
                        y=25
                    }
                }
                if(lvl=="ceceRoom_into_Kicten"){
                    if(x<=158 && obj=="dor1"){
                        x=159
                    }
                    if(y<=47){
                        y=48
                    }
                    if(x<=-13){
                        x=284
                        lvl="kitchen"
                    }
                    if(y>=59){
                        y=58
                    }
                    if(x>=291){
                        lvl="cece_room_2"
                        obj="animation"
                        x=30
                        y=64
                    }
                }
                if(lvl=="front_enterance1"){
                    if(y>=74){
                        if(x<=120){
                            x=121
                        }
                        if(x>=159){
                            x=158
                        }
                        if(y>=122){
                            lvl="hallway_to_front_enterance1"
                            y=-4
                        }
                    }
                    if(y<=74){
                        if(x<=55){
                            x=56
                        }
                        if(x>=227){
                            x=226
                        }
                        if(y<=20){
                            y=21
                        }
                    }
                }
                if(lvl=="hallway_to_front_enterance1"){
                    if(y<=-6){
                        lvl="front_enterance1"
                        y=112
                    }
                    if(y>=113){
