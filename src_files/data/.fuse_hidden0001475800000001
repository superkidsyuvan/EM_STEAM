            function touching(ax,ay,aw,ah,bx,by,bw,bh){
                if((ax + aw)>=bx && ax <=(bx+bw) && (ay+ah) > by && ay <= (by+bh)){
                    return true;
                }else{
                    return false;
                }
            }

            function get(object, key, default_value) {
                var result = object[key];
                return (typeof result !== "undefined") ? result : default_value;
            }

            function spawnOSU(letter,xPOS,yPOS){
                var oasdi = 0
                while(get(osu,letter+oasdi,'No')!='No'){
                    oasdi=oasdi+1 
                }
                osu[letter+oasdi]=0
                osuX[letter+oasdi]=xPOS
                osuY[letter+oasdi]=yPOS
                ctx.drawImage(document.getElementById("osucircle-"+IMAGEserver),osu[letter+oasdi],0,64,64,xPOS,yPOS,64,64)
                if(mode=="easy"){
                    ctx.drawImage(document.getElementById("letter_"+letter+"-"+IMAGEserver),xPOS,yPOS,64,64)
                }else{
                    ctx.drawImage(document.getElementById("icon_"+letter+"-"+IMAGEserver),xPOS,yPOS,64,64)
                }
            }

            async function playVideo() {
                try {
                    await document.getElementById('drive_clip-'+SOUNDserver).play();
                    console.log('diddidididi')
                } catch (err) {
                    return err
                    console.log(err)
                }
            }

