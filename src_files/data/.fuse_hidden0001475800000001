                this.computeFrame();
                let self = this;
                setTimeout(function () {
                    self.timerCallback();
                    }, 0);
                },
            
                doLoad: function() {
                this.video = document.getElementById("tsone_intro-imgur");
                this.c1 = document.getElementById("mainGame");
                this.ctx1 = this.c1.getContext("2d");
                //this.c2 = document.getElementById("c2");
                //this.ctx2 = this.c2.getContext("2d");
                let self = this;
                this.video.addEventListener("play", function() {
                    self.width = self.c1.width//self.video.videoWidth / 2;
                    self.height = self.c1.height//self.video.videoHeight / 2; (multi vid at once)
                    self.timerCallback();
                    }, false);
                },
            
                computeFrame: function() {
                if(lvl=="tsone_intro"){this.ctx1.drawImage(this.video, 0, 0, this.width, this.height);}
                let frame = this.ctx1.getImageData(0, 0, this.width, this.height);
                    let l = frame.data.length / 4;
                
                // below is for chroma key (would use movavi tho)    
                /*for (let i = 0; i < l; i++) {
                    let r = frame.data[i * 4 + 0];
                    let g = frame.data[i * 4 + 1];
                    let b = frame.data[i * 4 + 2];
                    if (g > 100 && r > 100 && b < 43)
                    frame.data[i * 4 + 3] = 0;
                }
                this.ctx2.putImageData(frame, 0, 0);*/
                return;
                }
            };
                        
            //universal functions (some left out cuz ya)
            function FRAME(){
                if(lvl!="osu!-doki_doki,[json-cece_F]"){ctx.clearRect(0,0,310, 150);}
                envo_2d()
                otherSTUF()
                envo_3d()
            }

            function aboveTEXTnotification(bgHEX,txtHEX,txtSTR){
                ctx.fillStyle=bgHEX;
                ctx.fillRect(10, 10, 270, 25);
                ctx.fillStyle=txtHEX;
                ctx.font = "10px Arial";
                ctx.fillText(txtSTR, 15, 30);
            }
            
            function addITEM(itemIMGname){
                for(var index = 0; index < boxINVENTORY.length; index++){
                    if(boxINVENTORY[index]==""){
                        boxINVENTORY[index]=itemIMGname;
                        return true;
                        break;
                    }
                    if(index==6){
                        if(boxINVENTORY[index]!=""){
                            return false;
                            break;
                        }
                    }
                }
            }

            /*function delITEM(itemIMGname){
                for(var index = 0; index < boxINVENTORY.length; index++){
                    if(boxINVENTORY[index]==itemIMGname){
                        boxINVENTORY[index]="";
                        pause=false
                        if(boxINVENTORY[0]=="" && boxINVENTORY[1]=="" && boxINVENTORY[3]=="" && boxINVENTORY[4]=="" && boxINVENTORY[5]==""){
                            inventory_toggle=false
                        }
                        return true;
                        break;
                    }
                    if(index==6){
                        if(boxINVENTORY[index]!=itemIMGname){
                            return false;
                            break;
                        }
                    }
                }
            }*/

            const intro_chapter1_desktop={
                timerCallback: function() {
                if (this.video.paused) {
                    return;
                }
                if(this.video.ended){
                    sound=false
                    lvl="soundin"
                    return "end";
                }
                this.computeFrame();
                let self = this;
                setTimeout(function () {
                    self.timerCallback();
                    }, 0);
                },
            
                doLoad: function() {
                this.video = document.getElementById("intro_chapter1-desktop");
                this.c1 = document.getElementById("mainGame");
                this.ctx1 = this.c1.getContext("2d");
                //this.c2 = document.getElementById("c2");
                //this.ctx2 = this.c2.getContext("2d");
                let self = this;
                this.video.addEventListener("play", function() {
                    self.width = self.c1.width//self.video.videoWidth / 2;
                    self.height = self.c1.height//self.video.videoHeight / 2; (multi vid at once)
                    self.timerCallback();
                    }, false);
                },
            
                computeFrame: function() {
                if(lvl=="chapter1_intro"){this.ctx1.drawImage(this.video, 0, 0, this.width, this.height);}
                let frame = this.ctx1.getImageData(0, 0, this.width, this.height);
                    let l = frame.data.length / 4;
                
                // below is for chroma key (would use movavi tho)    
                /*for (let i = 0; i < l; i++) {
                    let r = frame.data[i * 4 + 0];
                    let g = frame.data[i * 4 + 1];
                    let b = frame.data[i * 4 + 2];
                    if (g > 100 && r > 100 && b < 43)
                    frame.data[i * 4 + 3] = 0;
                }
                this.ctx2.putImageData(frame, 0, 0);*/
                return;
                }
            };

