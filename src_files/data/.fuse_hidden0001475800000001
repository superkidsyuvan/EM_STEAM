            function DrawPlayer(player,state,x,y,sx,sy){
                if(equiped=="y_yelKEY1"){
                    if(lvl!="garden_minigame"){
                        ctx.drawImage(document.getElementById("screen_yelKEY1-"+IMAGEserver),x,y)
                    }else{
                        ctx.drawImage(document.getElementById("screen_yelKEY1-"+IMAGEserver),136,65)
                    }
                }
                if(wannaDRAWme==true){
                    if(player=="bakaRUN-v1.2"){
                        if(state=="left"&&crouch==false){
                            ctx.drawImage(document.getElementById("runBaka_v1.2-"+IMAGEserver),frame_bg_main13,105,82,110,x,y,51,65)
                        }
                        if(state=="right"&&crouch==false){
                            ctx.drawImage(document.getElementById("runBaka_v1.2-"+IMAGEserver),frame_bg_main14,3,82,110,x,y,51,65)  
                        }
                        if(state=="up"&&crouch==false){
                            ctx.drawImage(document.getElementById("runBaka_v1.2-"+IMAGEserver),frame_bg_main15,136,32,94,x+13,y,25,65)  
                        }
                        if(state=="down"&&crouch==false){
                            ctx.drawImage(document.getElementById("runBaka_v1.2-"+IMAGEserver),frame_bg_main16,136,32,94,x+13,y,25,65)
                        }
                        if(state=="idle"&&crouch==false){
                            ctx.drawImage(document.getElementById("runBaka_v1.2-"+IMAGEserver),frame_bg_main17,3,32,94,x+13,y,25,65)
                        }
                        if(crouch==true){
                            ctx.drawImage(document.getElementById("runBaka_v1.2-"+IMAGEserver),frame_bg_main19,71,52,51,x+13,y,25,65)
                        }
                    }
                    collide()
                }
                if(player=="room1"){
