                    }, 0);
                },
            
                doLoad: function() {
                this.video = document.getElementById("cece_band_osu_playback1-imgur");
                this.c1 = document.getElementById("mainGame");
                this.ctx1 = this.c1.getContext("2d");
                //this.c2 = document.getElementById("c2");
                //this.ctx2 = this.c2.getContext("2d");
                let self = this;
                this.video.addEventListener("play", function() {
                    self.width = self.c1.width//self.video.videoWidth / 2;
                    self.height = self.c1.height//self.video.videoHeight / 2; (multi vid at once)
                    self.timerCallback();
                    }, false);
                },
            
                computeFrame: function() {
                if(lvl=="osu!-doki_doki,[json-cece_F]"){this.ctx1.drawImage(this.video, 0, 0, this.width, this.height);}
                let frame = this.ctx1.getImageData(0, 0, this.width, this.height);
                    let l = frame.data.length / 4;
                
                // below is for chroma key (would use movavi tho)    
                /*for (let i = 0; i < l; i++) {
                    let r = frame.data[i * 4 + 0];
                    let g = frame.data[i * 4 + 1];
                    let b = frame.data[i * 4 + 2];
                    if (g > 100 && r > 100 && b < 43)
                    frame.data[i * 4 + 3] = 0;
                }
                this.ctx2.putImageData(frame, 0, 0);*/

                if( Object.keys(osu).length===0){}else{
                    for (var [key, value] of Object.entries(osu)) {
                        ctx.drawImage(document.getElementById("osucircle-"+IMAGEserver),osu[key],0,64,64,osuX[key],osuY[key],64,64)
                        var m = key.slice(0,-1)
                        if(mode=="easy" && value<=960){
                            ctx.drawImage(document.getElementById("letter_"+m+"-"+IMAGEserver),osuX[key],osuY[key],64,64)
                        }else{
                            ctx.drawImage(document.getElementById("icon_"+m+"-"+IMAGEserver),osuX[key],osuY[key],64,64)
                        }
                        if(value==1024){
                            clicks_osu["miss"]=clicks_osu["miss"]+1
                            var oasdi = 0
                            var ee = key.slice(0,-1)
                            while(get(sta,ee+oasdi,'No')!='No'){
                                oasdi=oasdi+1 
                            }
                            staX[ee+oasdi]=osuX[key]
                            staY[ee+oasdi]=osuY[key]
                            staC[ee+oasdi]=0
                            sta[ee+oasdi]="miss"
                            delete osu[key]
                            delete osuX[key]
                            delete osuY[key]
                        } 
                    }
                }
                return;
                }
            };

