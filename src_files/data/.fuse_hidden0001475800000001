                            cece1_1="cece_sleeping"
                        }
                        ar="arrow_right"
                    }
                }
            })
            canvas.addEventListener("mousemove", function(e){
                if(lvl=="sound2"  || lvl=="sound3"|| lvl=="sound4"|| lvl=="sound5"){
                    if(e.clientX>=39 && e.clientX<=224 && e.clientY>=53 && e.clientY<=79){
                        lvl="sound3"
                    }else if(e.clientX>=38 && e.clientX<=191 && e.clientY>=110 && e.clientY<=133){
                        lvl="sound4"
                    }else if(e.clientX>=39 && e.clientX<=131 && e.clientY>=167 && e.clientY<=189){
                        lvl="sound5"
                    }else{
                        lvl="sound2"
                    }
                }
                if(lvl=="setting2" || lvl=="setting3" || lvl=="setting-so"){
                   if(e.clientX>=21 && e.clientX<=122 && e.clientY>=217 && e.clientY<=244){
                        lvl="setting3"
                    }else if(e.clientX>=132 && e.clientX<=175 && e.clientY>=45 && e.clientY<=62){
                        lvl="setting-so"
                    }else{
                        lvl="setting2"
                    }
                }
                if(lvl=="credit2" || lvl=="credit3"){
                   if(e.clientX>=350 && e.clientX<=448 && e.clientY>=33 && e.clientY<=58){
                        lvl="credit3"
                    }else{
                        lvl="credit2"
                    }
                }
                if(lvl=="introCUTP1-pause" || lvl=="introCUTP1"){
                	if(e.clientX>=425 && e.clientX<=495 && e.clientY>=229 && e.clientY<=250){
                        lvl="introCUTP1-pause"
                    }else{
                        lvl="introCUTP1"
                    }
                }
                if(lvl=="hallwayCHASE-start" || lvl=="hallwayCHASE-start-pause"){
                    if(e.clientX>=425 && e.clientX<=495 && e.clientY>=229 && e.clientY<=250){
                        lvl="hallwayCHASE-start-pause"
                    }else{
                        lvl="hallwayCHASE-start"
                    }
                }
                if(lvl=="menuSEL1" || lvl=="menuSEL1-rotate"){
                    if(e.clientX>=52 && e.clientX<=192 && e.clientY>=175 && e.clientY<=198){
                        lvl="menuSEL1-rotate"
                    }else{
                        lvl="menuSEL1"
                    } 
                }
                if(lvl=="menuSEL0" || lvl=="menuSEL0-rotate" || lvl=="menuSEL0-rotate1"){
                    if(e.clientX>=52 && e.clientX<=192 && e.clientY>=175 && e.clientY<=198){
                        lvl="menuSEL0-rotate"
                    }else if(e.clientX>=52 && e.clientX<=192 && e.clientY>=215 && e.clientY<=248){
                        if(tempINFO[3]==false){
                            lvl="menuSEL0-rotate1"
                        }
                    }else{
                        lvl="menuSEL0"
                    } 
                }
                if(lvl=="menuSEL1-difficulty_select" || lvl=="ms1dc" || lvl=="ms1dse" || lvl=="ms1dsn" || lvl=="ms1dsh"){
                    if(e.clientX>=340 && e.clientX<=373 && e.clientY>=21 && e.clientY<=56){
                        lvl="ms1dc"
                    }else if(e.clientX>=164 && e.clientX<=368 && e.clientY>=71 && e.clientY<=117){
                        lvl="ms1dse"
                    }else if(e.clientX>=166 && e.clientX<=367 && e.clientY>=134 && e.clientY<=179){
                        lvl="ms1dsn"
                    }else if(e.clientX>=164 && e.clientX<=366 && e.clientY>=193 && e.clientY<=239){
                        lvl="ms1dsh"
                    }else{
                        lvl="menuSEL1-difficulty_select"
                    } 
                }
                if(lvl=="plz_turn_on_sound_plzzz" || lvl=="plz_turn_on_sound_plzzz_decline:hover" || lvl=="plz_turn_on_sound_plzzz_accept:hover"){
                    //plz_turn_on_sound_plzzz_accept:hover
                    if(e.clientX>=293 && e.clientX<=351 && e.clientY>=117 && e.clientY<=130){
                        lvl="plz_turn_on_sound_plzzz_accept:hover"
                    }else if(e.clientX>=432 && e.clientX<=493 && e.clientY>=117 && e.clientY<=130){
                        lvl="plz_turn_on_sound_plzzz_decline:hover"
                    }else{
                        lvl="plz_turn_on_sound_plzzz"  
                    }
                }
                if(lvl=="cece_room_1_decide"){
                    if(e.clientX>=90 && e.clientX<=210 && e.clientY>=224 && e.clientY<=246){
                        decide_1=1
                    }else if(e.clientX>=310 && e.clientX<=420 && e.clientY>=223 && e.clientY<=245){
                        decide_1=2
                    }else{
                        decide_1=0
                    }
                }
                if(pause==true){
                    if(obj!=">:(-NOTI-INVENTORY-BAD-):<"){
                        if(char_pause.slice(-8)=="-statsss" || char_pause.slice(-8)=="-profile" || char_pause.slice(-8)=="-voiceln" ||char_pause.slice(-8)=="-charact"){
                            if(e.clientX>=10 && e.clientX<=66 && e.clientY>=205 && e.clientY<=235){
                                obj="resume_gamer"
                            }else if(e.clientX>=10 && e.clientX<=66 && e.clientY>=76 && e.clientY<=88){
                                obj="profile"
                            }else if(e.clientX>=8 && e.clientX<=68 && e.clientY>=93 && e.clientY<=116){
                                obj="voice_lines"
                            }else if(e.clientX>=10 && e.clientX<=65 && e.clientY>=120 && e.clientY<=133){
                                obj="stats"
                            }else if(e.clientX>=8 && e.clientX<=66 && e.clientY>=136 && e.clientY<=147){
                                obj="char"
                            }else{
                                obj="none"
                            }
                        }
                        if(char_pause.slice(-8)=="-basicin" || char_pause.slice(-8)=="-desc_in" || char_pause.slice(-8)=="-statsin" || char_pause.slice(-8)=="-previin" || char_pause.slice(-8)=="-advanin" || char_pause.slice(-8)=="-randoin" | char_pause.slice(-8)=="-itemtin"){
                            if(e.clientX>=6 && e.clientX<=141 && e.clientY>=61 && e.clientY<=76){
                                obj="basic_stuf_inven"
                            }else if(e.clientX>=9 && e.clientX<=83 && e.clientY>=109 && e.clientY<=95){
                                obj="description_inve"
                            }else if(e.clientX>=9 && e.clientX<=117 && e.clientY>=103 && e.clientY<=116){
                                obj="stats_inventoryy"
                            }else if(e.clientX>=7 && e.clientX<=130 && e.clientY>=123 && e.clientY<=135){
                                obj="pre-view_invento"
                            }else if(e.clientX>=7 && e.clientX<=127 && e.clientY>=142 && e.clientY<=155){
                                obj="advanced_invento"
                            }else if(e.clientX>=8 && e.clientX<=141 && e.clientY>=161 && e.clientY<=175){
                                obj="randomSHIT_inven"
                            }else if(e.clientX>=7 && e.clientX<=142 && e.clientY>=180 && e.clientY<=195){
                                obj="itemTYOE_invento"
                            }else if(e.clientX>=9 && e.clientX<=142 && e.clientY>=203 && e.clientY<=218){
                                obj="resume_gamer"
                            }else{
                                obj="none"
                            }  
                        } 
                        if(e.clientX>=7 && e.clientX<=126 && e.clientY>=7 && e.clientY<=22){
                            obj="profile_top"
                        }else if(e.clientX>=128 && e.clientX<=266 && e.clientY>=8 && e.clientY<=24){
                            obj="inventory_top"
                        }else if(e.clientX>=271 && e.clientX<=295 && e.clientY>=8 && e.clientY<=24){
                            obj="menuin_top"
                        }
                    };
                };
                if(lvl=="menuSEL1-locked" || lvl=="menuSEL1-locked2"){
                    if(e.clientX>=181 && e.clientX<=444 && e.clientY>=177 && e.clientY<=211){
                        lvl="menuSEL1-locked2"
                    }else{
                        lvl="menuSEL1-locked"
                    } 
                }
                if(inventory_toggle==true){
                    if(e.clientX>=94 && e.clientX<=139 && e.clientY>=204 && e.clientY<=250){
                        ctx.drawImage(document.getElementById("inventory_hover-"+IMAGEserver),50,117,32,32)
                        INVENTORYhov[0]="1";
                    }else if(e.clientX>=148 && e.clientX<=195 && e.clientY>=205 && e.clientY<=251){
                        ctx.drawImage(document.getElementById("inventory_hover-"+IMAGEserver),83,117,32,32)
                        INVENTORYhov[1]="2";
                    }else if(e.clientX>=202 && e.clientX<=251 && e.clientY>=207 && e.clientY<=251){
                        ctx.drawImage(document.getElementById("inventory_hover-"+IMAGEserver),116,117,32,32)
                        INVENTORYhov[2]="3";
                    }else if(e.clientX>=259 && e.clientX<=306 && e.clientY>=203 && e.clientY<=251){
                        ctx.drawImage(document.getElementById("inventory_hover-"+IMAGEserver),149,117,32,32)
                        INVENTORYhov[3]="4";
                    }else if(e.clientX>=314 && e.clientX<=362 && e.clientY>=203 && e.clientY<=251){
                        ctx.drawImage(document.getElementById("inventory_hover-"+IMAGEserver),182,117,32,32)
                        INVENTORYhov[4]="5";
                    }else if(e.clientX>=368 && e.clientX<=417 && e.clientY>=203 && e.clientY<=251){
                        ctx.drawImage(document.getElementById("inventory_hover-"+IMAGEserver),215,117,32,32)
                        INVENTORYhov[5]="6";
                    }else{
                        INVENTORYhov[0]="";
                        INVENTORYhov[1]="";
                        INVENTORYhov[2]="";
                        INVENTORYhov[3]="";
                        INVENTORYhov[4]="";
                        INVENTORYhov[5]="";
                    }
                }
            })

            document.getElementById('tsone_intro-google').onended = (event) => {
                console.log("tsone_intro-google stopped either because it has finished playing or no further data is available.");
                sound=false
                lvl="soundin"
            };
            document.getElementById('tsone_intro-imgur').onended = (event) => {
                console.log("tsone_intro-imgur stopped either because it has finished playing or no further data is available.");
                sound=false
                lvl="soundin"
            };  

            document.getElementById('intro_chapter1-google').onended = (event) => {
                console.log("intro_chapter1-google stopped either because it has finished playing or no further data is available.");
                lvl="hallwayCHASE-0"
            };

            document.getElementById('intro_chapter1-imgur').onended = (event) => {
                console.log("intro_chapter1-imgur stopped either because it has finished playing or no further data is available.");
                lvl="hallwayCHASE-0"
            };
            
            document.getElementById('cece_band_osu_playback-google').onended = (event) => {
                //lvl="drive_clip_osu_ended_"
                lvl="osu_afterplay_stats"
                document.getElementById('cece_band_osu_playback-google').pause()
                document.getElementById('drive_clip-drive').pause()
                document.getElementById('drive_clip-SOUNDdesk').pause()
                document.getElementById('drive_clip-r3').pause()
                document.getElementById('cece_band_osu_playback-google').currentTime=0
            };
            
            document.getElementById('cece_band_osu_playback1-imgur').onended = (event) => {
                document.getElementById('cece_band_osu_playback2-imgur').play()
                cece_band_osu_playback2_imgur.doLoad()
            };
            
            document.getElementById('cece_band_osu_playback2-imgur').onended = (event) => {
                lvl="osu_afterplay_stats"
                document.getElementById('cece_band_osu_playback2-imgur').pause()
                document.getElementById('drive_clip-drive').pause()
                document.getElementById('drive_clip-SOUNDdesk').pause()
                document.getElementById('drive_clip-r3').pause()
                document.getElementById('cece_band_osu_playback2-imgur').currentTime=0
            };
