            window.addEventListener("keyup", function(e){
                pressed.push(e.key);
                pressed.splice(-secretCODE.length-1,pressed.length-secretCODE.length);
                if(pressed.join("").includes(secretCODE)){
                    console.log("yes, edwerd did eat the banana")
                    lvl="question0-s"
                }
                if(e.key=="-"){
                    if(sound==true || SOUNDserver=="SOUNDdesk"){
                        if(document.querySelector('audio')!=0){
                            document.querySelector('audio').volume=document.querySelector('audio').volume-0.1
                            document.querySelector('video').volume=document.querySelector('video').volume-0.1
                            document.getElementById("ding-"+SOUNDserver).play()
                        }
                    }
                }
                if(e.key=="=" || e.key=="+"){
                    if(sound==true || SOUNDserver=="SOUNDdesk"){
                        if(document.querySelector('audio')!=1){
                            document.querySelector('audio').volume=document.querySelector('audio').volume+0.1
                            document.querySelector('video').volume=document.querySelector('video').volume+0.1
                            document.getElementById("ding-"+SOUNDserver).play()
                        }
                    }
                }
                if(lvl=="start-img"){
                    if(e.key=="I" || e.key=="i"){
                        IMAGEserver="imgur"
                        lvl="load"
                    }
                    if(e.key=="G" || e.key=="g"){
                        IMAGEserver="google"
                        lvl="load"
                    }
                    if(e.key=="D" || e.key=="d"){
                        if(document.getElementById("external_script").innerHTML=="true"){IMAGEserver="desktop"}
                        lvl="load"
                        
                    }
                }
                if(lvl=="question1-s"){
                    if(e.key=="a"){
                        lvl="winner-s-1"
                    }
                    if(e.key=="b" || e.key=="c" || e.key=="b"){
                        frame_bg_main12=0
                        lvl="loser-s-1"
                    }
                }
                if( Object.keys(osu).length===0){}else{
                    for (var [key, value] of Object.entries(osu)) {
                        var m = key.slice(0,-1)
                        if(e.key=m){
                            console.log("key: "+key)
                            /*var val_x = osuX[key]
                            var val_y = osuY[key]
                            var horrible = clicks_osu["horrible"]
                            var bad = clicks_osu["bad"]
                            var mid = clicks_osu["mid"]
                            var good = clicks_osu["good"]
                            var perfect = clicks_osu["perfect"]*/
                            var oasdi = 0
                            var ee = key.slice(0,-1)
                            while(get(sta,ee+oasdi,'No')!='No'){
                                oasdi=oasdi+1 
                            }
                            staX[ee+oasdi]=osuX[key]
                            staY[ee+oasdi]=osuY[key]
                            staC[ee+oasdi]=0
                            if(osu[key]<=960 && osu[key]>=832){
                                clicks_osu["horrible"]=clicks_osu["horrible"]+1
                                sta[ee+oasdi]="horrible"
                            }
                            if(osu[key]<=768 && osu[key]>=640){
                                clicks_osu["bad"]=clicks_osu["bad"]+1
                                sta[ee+oasdi]="bad"
                            }
                            if(osu[key]<=448 && osu[key]>=320){
                                clicks_osu["mid"]=clicks_osu["mid"]+1
                                sta[ee+oasdi]="mid"
                            }
                            if(osu[key]<=256 && osu[key]>=128){
                                clicks_osu["good"]=clicks_osu["good"]+1
                                sta[ee+oasdi]="good"
                            }
                            if(osu[key]<=64 && osu[key]>=0){
                                clicks_osu["perfect"]=clicks_osu["perfect"]+1
                                sta[ee+oasdi]="perfect"
                            }
                            delete osu[key]
                            delete osuX[key]
                            delete osuY[key]
                            break;
                        }
                    }
                }
                if(wannaDRAWme==true&&jump==false){
                    if(sound==true || SOUNDserver=="SOUNDdesk"){document.getElementById("walking-"+SOUNDserver).play()}
                    moveDIR="idle"
                }
                if(e.key=="Control"){
                    crouch=false
                }
                if(e.key=="c"){
                	if(crouch==true){
                    	crouch=false
                        return false
                    }else{
                    	crouch=true
                        return true
                    }
