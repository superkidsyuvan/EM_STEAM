                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),17,17,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_up_chart"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),34,17,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_right_chart"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),51,17,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_left_miss"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),0,34,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_down_miss"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),17,34,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_up_miss"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),34,34,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_right_miss"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),51,34,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_left_note_press"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),0,51,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_down_note_press"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),17,51,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_up_note_press"){
                        ctx.drawImage(document.getElementById("arrow1-"+IMAGEserver),34,51,17,17,x,y,34,34)  
                    }
                    if(state=="arrow_right_note_press"){
