            //below is 3D processing 

            //variables & constants (very important)

            //map variables & constants
            const MAP_SIZE = 32;
            const MAP_SCALE = 64;
            const MAP_RANGE = MAP_SCALE * MAP_SIZE;
            const MAP_SPEED = (MAP_SCALE / 2) / 10;
            var map = [
