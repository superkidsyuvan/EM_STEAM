            function otherSTUF(){
                if(lvl=="osu!-doki_doki,[json-cece_F]"){
                    if(document.getElementById("drive_clip-"+SOUNDserver).currentTime=0){

                    }
                }
                if(lvl=="garden_minigame"){
                    if(NPCcount==0){
                        NPCmoveDIR="left"
                        dx =dx+2
                        if(dx==550){
                            NPCcount=1
                            NPCmoveDIR="idle"
                            setTimeout(function(){
                                NPCcount=4
                            },100)
                        }
                    }
                    if(NPCcount==1){
                        NPCmoveDIR="idle"
                    }
                    if(NPCcount==2){
                        NPCmoveDIR="right"
                        dx=dx-2
                        if(dx==100){
                            NPCcount=1
                            setTimeout(function(){
                                NPCcount=3
                            },100)
                        }
                    }
                    if(NPCcount==3){
                        NPCmoveDIR="up"
                        dy=dy-2 
                        if(dy==50){
                            NPCcount=1
                            setTimeout(function(){
                                NPCcount=0
                            },100)
                        }
                    }
                    if(NPCcount==4){
                        NPCmoveDIR="down"
                        dy =dy+2
                        if(dy==200){
                            NPCcount=1
                            setTimeout(function(){
                                NPCcount=2
                            },100)
                        }
                    }
                }
                if(fnf_mode==true){
                    if(alc==true){
                        if(mode=="hard" || mode=="norm"){
                            alcp=alcp+9
                        }else{
                            alcp=alcp+3
                        }
                    }
                    if(alcp==150){
                        alc=false
                        alcp=-34
                    }
                    if(arc==true){
                        if(mode=="hard" || mode=="norm"){
                            arcp=arcp+9
                        }else{
                            arcp=arcp+3
                        }
                    }
                    if(arcp==150){
                        arc=false
                        arcp=-34
                    }
                    if(auc==true){
                        if(mode=="hard" || mode=="norm"){
                            aucp=aucp+9
                        }else{
                            aucp=aucp+3
                        }
                    }
                    if(aucp==150){
                        auc=false
                        aucp=-34
                    }
                    if(adc==true){
                        if(mode=="hard" || mode=="norm"){
                            adcp=adcp+9
                        }else{
                            adcp=adcp+3
                        }
                    }
                    if(adcp==150){
                        adc=false
                        adcp=-34
                    }
                }
            }

            function envo_2d(){
                if(lvl=="start-warn"){
                    ctx.font = "10px Arial";
                    ctx.fillStyle = "white";
                    ctx.strokeStyle = "white";
                    ctx.fillText("This game requires a keyboard and a mouse.", 10, 120);
                    ctx.fillText("Click to continue...", 10, 140);
                    ctx.strokeRect(5,5,100,50)
                    ctx.fillText("keyboard here", 20, 20);
                    ctx.beginPath()
                    ctx.ellipse(150, 60, 30, 55, 0, 0, 2 * Math.PI);
                    ctx.stroke()
                    ctx.fillText("mouse here", 123, 70);
                }
                if(lvl=="start-img"){
                    ctx.font = "10px Arial";
                    ctx.fillStyle = "white";
                    ctx.strokeStyle = "white";
                    ctx.fillText("press 'I' for imgur image server", 10, 10);
                    ctx.fillText("press 'G' for google image server", 10, 30);
                    if(document.getElementById("external_script").innerHTML=="true"){ctx.fillText("press 'D' for desktop image server", 10, 70);}
                }
                if(lvl=="load"){
                    ctx.font = "15px Arial";
                    ctx.fillStyle="white";
                    var loade = load.toString();
                    var ss = "Loading Assets ("+loade+"/400)";
                    ctx.fillText(ss, 10, 50);
                    var mma = Number(intake)
                    if(mma<=load){
                        lvl="plz_turn_on_sound_plzzz"
                        //lvl="garden_minigame"
                        //type="evil"
                    }
                }
                if(lvl=="enter"){
                    ctx.drawImage(document.getElementById("enter1-"+IMAGEserver),frame_bg_main,0,256,144,0,0,310,150)
                }
                if(lvl=="sound"){
                    ctx.drawImage(document.getElementById("sound1-"+IMAGEserver),frame_bg_main1,0,256,144,0,0,310,150)
                }
                if(lvl=="sound2"){
                    ctx.drawImage(document.getElementById("sound2-"+IMAGEserver),0,0,310,150)
                }
                if(lvl=="sound3"){
                    ctx.drawImage(document.getElementById("sound3-"+IMAGEserver),0,0,310,150)
                }
                if(lvl=="sound4"){
                    ctx.drawImage(document.getElementById("sound4-"+IMAGEserver),0,0,310,150)
                }
                if(lvl=="sound5"){
                    ctx.drawImage(document.getElementById("sound5-"+IMAGEserver),0,0,310,150)
                }
                if(lvl=="setting1"){
                    ctx.drawImage(document.getElementById("setting1-"+IMAGEserver),frame_bg_main2,0,256,144,0,0,310,150)
                }
                if(lvl=="settingb"){
                    ctx.drawImage(document.getElementById("setting4-"+IMAGEserver),frame_bg_main3,0,256,144,0,0,310,150)
                }
                if(lvl=="setting2"){
                    ctx.drawImage(document.getElementById("setting2-"+IMAGEserver),0,0,310,150)
                    if(sound==false){
                        ctx.drawImage(document.getElementById("off-"+IMAGEserver),75,23,26,10)
                    }
                    if(sound==true || SOUNDserver=="SOUNDdesk"){
                        ctx.drawImage(document.getElementById("on-"+IMAGEserver),75,23,26,10)
                    }
                }
                if(lvl=="setting-so"){
                    ctx.drawImage(document.getElementById("setting2-"+IMAGEserver),0,0,310,150)
                    if(sound==false){
                        ctx.drawImage(document.getElementById("off-"+IMAGEserver),72,20,32,16)
                    }
                    if(sound==true || SOUNDserver=="SOUNDdesk"){
                        ctx.drawImage(document.getElementById("on-"+IMAGEserver),72,20,32,16)
                    }
                }
                if(lvl=="setting3"){
                    ctx.drawImage(document.getElementById("setting3-"+IMAGEserver),0,0,310,150)
                    if(sound==false){
                        ctx.drawImage(document.getElementById("off-"+IMAGEserver),75,23,26,10)
                    }
                    if(sound==true || SOUNDserver=="SOUNDdesk"){
                        ctx.drawImage(document.getElementById("on-"+IMAGEserver),75,23,26,10)
                    }
                }
                if(lvl=="soundin"){
                    ctx.drawImage(document.getElementById("soundin-"+IMAGEserver),frame_bg_main8,0,256,144,0,0,310,150)
                }
                if(lvl=="soundout"){
                    ctx.drawImage(document.getElementById("soundout-"+IMAGEserver),frame_bg_main9,0,256,144,0,0,310,150)
                }
                if(lvl=="soundload"){
                    ctx.drawImage(document.getElementById("soundload-"+IMAGEserver),0,0,310,150)
                    ctx.fillStyle = "white"
                    ctx.font = "15px Arial";
                    var backup = "Loading Sounds ("+SOUNDLOAD+"/"+all_sounds.toString()+")"
                    ctx.fillText(backup, 35, 100);
                    if(SOUNDserver=="drive"){
                        if((all_sounds/2)<=SOUNDload){
                            lvl="soundout"
                            document.getElementById("ambience-"+SOUNDserver).play()
                        }
                    }else{
                        if(all_sounds==SOUNDload){
                            lvl="soundout"
                            document.getElementById("ambience-"+SOUNDserver).play()
                        }
                    }
                }
                if(lvl=="warning1"){
                    ctx.drawImage(document.getElementById("warning1-"+IMAGEserver),frame_bg_main4,0,256,144,0,0,310,150)
                }
                if(lvl=="warning2"){
                    ctx.drawImage(document.getElementById("warning2-"+IMAGEserver),-10,0,310,150)
                }
                if(lvl=="warning3"){
                    ctx.drawImage(document.getElementById("warning3-"+IMAGEserver),frame_bg_main5,0,256,144,0,0,310,150)
                }
                if(lvl=="credit1"){
                    ctx.drawImage(document.getElementById("intoCRE-"+IMAGEserver),frame_bg_main6,0,256,144,0,0,310,150)
                }
                if(lvl=="credit2"){
                    ctx.drawImage(document.getElementById("credit-"+IMAGEserver),0,0,310,150)
                }
                if(lvl=="credit3"){
                    ctx.drawImage(document.getElementById("credit2-"+IMAGEserver),0,0,310,150)
                }
                if(lvl=="credit4"){
                    ctx.drawImage(document.getElementById("outoCRE-"+IMAGEserver),frame_bg_main7,0,256,144,0,0,310,150)
                }
                if(lvl=="introCUTP1"){
                    ctx.drawImage(document.getElementById("introS1P1-"+IMAGEserver),frame_bg_main10,0,256,144,0,0,300,150)
                    ctx.drawImage(document.getElementById("skip1-"+IMAGEserver),250,130)
                }
                if(lvl=="introCUTP1-pause"){
                 ctx.drawImage(document.getElementById("introS1P1-"+IMAGEserver),frame_bg_main10,0,256,144,0,0,300,150)
                    ctx.drawImage(document.getElementById("skip1-"+IMAGEserver),240,120,70,25)
                    }
                if(lvl=="introCUTP2"){
                    ctx.drawImage(document.getElementById("introS1P2-"+IMAGEserver),frame_bg_main11,0,256,144,0,0,300,150)
                }
                if(lvl=="hallwayCHASE-start"){
                    ctx.drawImage(document.getElementById("hallway-"+IMAGEserver),0,0,256,144,0,0,300,150);
                    DrawPlayer("harvester_chase_scene","idle_norm",20,65,0,0)
                    DrawPlayer(player,moveDIR,220,80,0,0);
                    ctx.drawImage(document.getElementById("skip1-"+IMAGEserver),250,130)
                }
                if(lvl=="hallwayCHASE-start-pause"){
                    ctx.drawImage(document.getElementById("hallway-"+IMAGEserver),0,0,256,144,0,0,300,150);
                    DrawPlayer("harvester_chase_scene","idle_norm",20,65,0,0)
                    DrawPlayer(player,moveDIR,220,80,0,0);
                    ctx.drawImage(document.getElementById("skip1-"+IMAGEserver),240,120,70,25)   
                }
                if(lvl=="hallwayCHASE-into_craz"){
                    ctx.drawImage(document.getElementById("hallway-"+IMAGEserver),0,0,256,144,0,0,300,150);
                    DrawPlayer("harvester_chase_scene","into_craz",15,65,0,0)
                    DrawPlayer(player,moveDIR,220,80,0,0);   
                }
                if(lvl=="hallwayCHASE-idle_craz"){
                    ctx.drawImage(document.getElementById("hallway-"+IMAGEserver),0,0,256,144,0,0,300,150);
                    DrawPlayer("harvester_chase_scene","idle_craz",15,65,0,0)
                    DrawPlayer(player,moveDIR,220,80,0,0);   
                }
                if(lvl=="hallwayCHASE-laugh"){
                    ctx.drawImage(document.getElementById("hallway-"+IMAGEserver),0,0,256,144,0,0,300,150);
                    DrawPlayer("harvester_chase_scene","laughing",15,65,0,0)
                    DrawPlayer(player,moveDIR,220,80,0,0);   
                }
                if(lvl=="hallway-demo1"){
                    ctx.drawImage(document.getElementById("hallway-"+IMAGEserver),0-x,0-y,2358,150);
                    DrawPlayer(player,moveDIR,155,75,0,0);
                    DrawPlayer("harvester_chase_scene","running",x-lookbehindyou,65,0,0)
                }  
                if(lvl=="hallwayCHASE-1"){
                    ctx.drawImage(document.getElementById("hallway-"+IMAGEserver),0,0,256,144,0,0,300,150);
                    DrawPlayer(player,moveDIR,x,y,0,0);
                    DrawPlayer("harvester_chase_scene","running",x-lookbehindyou,65,0,0)   
