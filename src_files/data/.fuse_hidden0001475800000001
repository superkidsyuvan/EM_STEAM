            document.getElementById("ready-drive").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=1}}
            document.getElementById("set-drive").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=2}}
            document.getElementById("go-drive").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=0}}
            document.getElementById("ready-SOUNDdesk").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=1}}
            document.getElementById("set-SOUNDdesk").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=2}}
            document.getElementById("go-SOUNDdesk").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=0}}
            document.getElementById("ready-r3").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=1}}
            document.getElementById("set-r3").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=2}}
            document.getElementById("go-r3").onended=function(){if(lvl=='hallwayCHASE-0'){rsg=0}}
            document.getElementById("laugh-drive").onended=function(){if(lvl=="hallwayCHASE-laugh"){lvl="hallwayCHASE-1";x=80;document.getElementById("chasemusic-"+SOUNDserver).play()}}
            document.getElementById("begin-drive").onended=function(){if(lvl=="hallwayCHASE-start" || lvl=="hallwayCHASE-start-pause"){document.getElementById("laugh-"+SOUNDserver).play();lvl="hallwayCHASE-laugh"}}
            document.getElementById("decide-drive").onended=function(){if(lvl=="cece_room_1_decide"){lvl="she_is_the_ded"}}
            document.getElementById("laugh-r3").onended=function(){if(lvl=="hallwayCHASE-laugh"){lvl="hallwayCHASE-1";x=80;document.getElementById("chasemusic-"+SOUNDserver).play()}}
