            function amough(){
                tempINFO[6]=1
            }

            function amough2(){
                tempINFO[7]=1
            }

            function typewriter(text,xPOS,yPOS){
                dbTW=true
                nuTW=nuTW+1
                ctx.fillText(text.substring(0,nuTW), xPOS, yPOS);
                dbTW=false
            }

            function dialogue(){
                    ctx.fillStyle="black";
                    ctx.fillRect(10, 95, 270, 50);
                    if(lvl=="doyoulife"){
                        if(dia==0){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("cece1","cece_profilepic1",10,28)
                            typewriter("...", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==1){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("harvester_chase_scene","profile",10,28)
                            typewriter("...", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==2){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("cece1","cece_profilepic1",10,28)
                            typewriter("........", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==3){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("harvester_chase_scene","profile",10,28)
                            typewriter("..................", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==4){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("harvester_chase_scene","profile",10,28)
                            typewriter("dont worry. i fixed window!!", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==5){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("cece1","cece_profilepic1",10,28)
                            typewriter("ok", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==6){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("harvester_chase_scene","profile",10,28)
                            typewriter("...", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==7){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("harvester_chase_scene","profile",10,28)
                            typewriter("doyoulife?", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                    }
                    if(lvl=="she_evil_shit"){
                        if(dia==0){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("cece1","cece_profilepic2",10,28)
                            typewriter("i col now ", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==1){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("harvester_chase_scene","profile",10,28)
                            typewriter("very", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==2){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("harvester_chase_scene","profile",10,28)
                            typewriter("can me take control of body?", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==3){
                            ctx.fillStyle="white"; 
                            ctx.font = "15px Arial";
                            DrawPlayer("cece1","cece_profilepic2",10,28)
                            ctx.fillText("but make sure to be cool ", 15, 110);
                            ctx.font = "10px Arial";
                            ctx.fillText("Click To Continue", 15, 130);
                        }
                        if(dia==4){
                            ctx.fillStyle="white"; 
