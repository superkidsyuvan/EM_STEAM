                        lvl="cecequestintro"
                        player="cece1"
                        document.getElementById("chasemusic-drive").pause() 
                        document.getElementById("chasemusic-drive").currentTime=0
                    }
                }
                if(lvl=="hallwayCHASE-8"){
                    if(y<=50&&jump==false){
                        y=53
                    }
                    if(y<=89){
                        y=86
                    }
                    if(x>=243){
                        x=240
                    }
                }
                if(lvl=="hallwayCHASE-6"){
                    if(x>=276){
                        lvl="hallwayCHASE-8"
                        x=-21
                    }
                    if(y<=37){
                        y=34
                    }
                    if(y<=89&&jump==false){
